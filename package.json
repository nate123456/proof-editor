{
  "name": "proof-editor",
  "displayName": "Proof Editor",
  "description": "Visual formal reasoning tool for constructing logical arguments",
  "version": "0.1.0",
  "publisher": "proof-editor-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/proof-editor/proof-editor"
  },
  "type": "module",
  "engines": {
    "vscode": "^1.74.0",
    "node": ">=22.0.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./tsconfig.extension.json",
    "watch": "tsc -watch -p ./tsconfig.extension.json",
    "build": "tsc --noEmit",
    "dev": "tsc --noEmit --watch",
    "type-check": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@faker-js/faker": "^9.8.0",
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.74.0",
    "@vitest/coverage-v8": "^3.2.4",
    "fast-check": "^4.1.1",
    "fishery": "^2.3.1",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "dependencies": {}
}
